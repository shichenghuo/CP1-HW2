function setup() {
    createCanvas(400, 400);
    background(220);
    // HSB with H range 0-400, S range 0-400, B range 0-100:
    colorMode(HSB, 400, 400, 100);
}
// No matter how the name change, the function will not be influenced
function draw() {
    let Cats = random(width);
    let Dogs = random(height);
    fill( Cats, Dogs, 100);
    noStroke()
    ellipse( Cats, Dogs, 20);
}
